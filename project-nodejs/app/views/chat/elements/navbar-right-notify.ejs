%> 
<li class="dropdown notifications-menu">
    <!-- Menu toggle button -->
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
        <i class="fa fa-fw fa-user-plus"></i>
        <span class="label label-warning total-user-invite"><%= userInfo.totalRequest %></span>
    </a>
    <ul class="dropdown-menu">
        <%- showBoxRequestFriend(userInfo.totalRequest, userInfo.requestFrom) %> 
    </ul>
</li>